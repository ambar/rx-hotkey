{
  "name": "rx-hotkey",
  "version": "0.0.2",
  "description": "A simple hotkey library",
  "license": "MIT",
  "repository": "ambar/rx-hotkey",
  "module": "src/hotkey.js",
  "main": "dist/hotkey.js",
  "scripts": {
    "prepare": "npm test && npm run build",
    "start": "docz dev",
    "test": "jest --coverage",
    "test:watch": "jest --watch --notify",
    "build": "rollup -f cjs $npm_package_module -o $npm_package_main"
  },
  "files": [
    "dist",
    "src"
  ],
  "jest": {
    "preset": "rollup-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.docz/"
    ]
  },
  "peerDependencies": {
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "codecov": "^3.1.0",
    "dialog-polyfill": "^0.4.10",
    "docz": "^2.3.1",
    "jest": "^26.6.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-spring": "^8.0.7",
    "rollup": "^2.38.4",
    "rollup-jest": "^1.1.1",
    "rxjs": "^6.4.0"
  }
}
